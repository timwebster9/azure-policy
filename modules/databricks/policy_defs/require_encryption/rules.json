{
    "if": {
        "allOf": [
            {
                "field": "type",
                "equals": "Microsoft.Databricks/workspaces"
            },
            {
                "field": "Microsoft.Databricks/workspaces/parameters.requireInfrastructureEncryption",
                "notEquals": "true"
            }
        ]
    },
    "then": {
        "effect": "[parameters('effect')]"
    }
}