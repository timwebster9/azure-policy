{
    "if": {
        "allOf": [
            {
                "field": "type",
                "equals": "Microsoft.EventHub/Namespaces/NetworkRuleSets"
            },
            {
                "not": {
                    "field": "Microsoft.EventHub/Namespaces/NetworkRuleSets/defaultAction",
                    "equals": "Deny"
                }
            }
        ]
    },
    "then": {
        "effect": "[parameters('effect')]"
    }
}