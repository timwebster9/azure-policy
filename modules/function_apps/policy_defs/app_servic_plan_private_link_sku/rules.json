{
    "if": {
        "allOf": [
            {
                "field": "type",
                "equals": "Microsoft.Web/serverFarms"
            },
            {
                "field": "Microsoft.Web/serverFarms/sku.tier",
                "notIn": [
                    "PremiumV2",
                    "PremiumV3",
                    "ElasticPremium"
                ]
            }
        ]
    },
    "then": {
        "effect": "[parameters('effect')]"
    }
}