{
    "if": {
        "allOf": [
            {
                "field": "location",
                "notIn": "[parameters('listOfRegionsAllowed')]"
            },
            {
                "field": "type",
                "notIn": [
                    "Microsoft.Security/pricings",
                    "Microsoft.DataFactory/factories/datasets",
                    "Microsoft.DataFactory/factories/pipelines"
                ]
            }
        ]
    },
    "then": {
        "effect": "[parameters('effect')]"
    }
}